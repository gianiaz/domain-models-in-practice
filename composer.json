{
  "name": "gianiaz/domain-models-in-practice",
  "description": "Domain models in practice - A PHP prototypical implementation",
  "type": "project",
  "authors": [
    {
      "name": "Giovanni Lenoci",
      "email": "gianiaz@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "behat/behat": "^3.8",
    "ramsey/uuid": "^4.1",
    "thecodingmachine/safe": "^1.3",
    "webmozart/assert": "^1.9"
  },
  "require-dev": {
    "facile-it/facile-coding-standard": "dev-master#5b3fd61bb8e05dd544ca9d95f583ae86d2b90ff6 as 0.4.1",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^0.12.74",
    "phpstan/phpstan-phpunit": "^0.12.17",
    "phpstan/phpstan-webmozart-assert": "^0.12.12",
    "phpunit/phpunit": "^9.5",
    "psalm/plugin-phpunit": "^0.15.1",
    "roave/security-advisories": "dev-master",
    "vimeo/psalm": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "DDD\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DDD\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.0.3"
    },
    "sort-packages": true
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --dry-run --diff",
    "cs-fix": "php-cs-fixer fix --diff"
  }
}
